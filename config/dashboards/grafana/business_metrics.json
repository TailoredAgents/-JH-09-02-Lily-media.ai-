{
  "id": null,
  "title": "Lily Media AI - Business Metrics",
  "tags": ["business", "kpis", "revenue"],
  "style": "dark",
  "timezone": "browser",
  "refresh": "30s",
  "schemaVersion": 39,
  "fiscalYearStartMonth": 0,
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Active Users",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(increase(user_registrations_total[24h]))",
          "legendFormat": "New Users (24h)",
          "refId": "A"
        },
        {
          "expr": "user_active_sessions",
          "legendFormat": "Active Sessions",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "short",
          "min": 0
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 2,
      "title": "Revenue Metrics",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(subscription_revenue_total[1d])) * 86400",
          "legendFormat": "Daily Revenue",
          "refId": "A"
        },
        {
          "expr": "sum(subscription_active_total)",
          "legendFormat": "Active Subscriptions",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "currencyUSD",
          "min": 0
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 3,
      "title": "Plan Distribution",
      "type": "piechart",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum by (plan_name) (subscription_active_total)",
          "legendFormat": "{{ plan_name }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "short"
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "pieType": "pie",
        "tooltip": {
          "mode": "single"
        },
        "legend": {
          "displayMode": "list",
          "placement": "right"
        }
      }
    },
    {
      "id": 4,
      "title": "Content Generation Usage",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(content_generation_requests_total[1h]))",
          "legendFormat": "Content Requests/hr",
          "refId": "A"
        },
        {
          "expr": "sum(rate(image_generation_requests_total[1h]))",
          "legendFormat": "Image Requests/hr",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "reqps",
          "min": 0
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 5,
      "title": "Feature Usage Over Time",
      "type": "timeseries",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "sum(rate(content_generation_requests_total[5m]))",
          "legendFormat": "Content Generation",
          "refId": "A"
        },
        {
          "expr": "sum(rate(image_generation_requests_total[5m]))",
          "legendFormat": "Image Generation",
          "refId": "B"
        },
        {
          "expr": "sum(rate(research_requests_total[5m]))",
          "legendFormat": "Research Requests",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "reqps",
          "min": 0
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        },
        "legend": {
          "calcs": [],
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 6,
      "title": "Revenue Trends",
      "type": "timeseries",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "sum(rate(subscription_revenue_total[1h])) * 3600",
          "legendFormat": "Hourly Revenue",
          "refId": "A"
        },
        {
          "expr": "sum(increase(subscription_revenue_total[24h:1h]))",
          "legendFormat": "Daily Revenue (24h ago)",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "currencyUSD",
          "min": 0
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        },
        "legend": {
          "calcs": [],
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 7,
      "title": "User Engagement Metrics",
      "type": "table",
      "gridPos": {
        "h": 9,
        "w": 24,
        "x": 0,
        "y": 17
      },
      "targets": [
        {
          "expr": "sum by (user_plan) (rate(content_generation_requests_total[1h]))",
          "legendFormat": "{{ user_plan }}",
          "refId": "A",
          "format": "table"
        },
        {
          "expr": "sum by (user_plan) (rate(image_generation_requests_total[1h]))",
          "legendFormat": "{{ user_plan }}",
          "refId": "B",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "unit": "reqps"
        }
      },
      "options": {
        "showHeader": true
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            },
            "indexByName": {},
            "renameByName": {
              "user_plan": "Plan",
              "Value #A": "Content Requests/hr",
              "Value #B": "Image Requests/hr"
            }
          }
        }
      ]
    }
  ],
  "templating": {
    "list": []
  },
  "annotations": {
    "list": [
      {
        "name": "Deployments",
        "datasource": "Prometheus",
        "enable": true,
        "expr": "changes(deployment_info[1h])",
        "iconColor": "green",
        "titleFormat": "Deployment",
        "textFormat": "New version deployed"
      }
    ]
  }
}